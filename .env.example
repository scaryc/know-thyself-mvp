# Database Configuration
# Using SQLite for development (migrate to PostgreSQL for production)
DATABASE_URL="file:./dev.db"

# For PostgreSQL (production):
# DATABASE_URL="postgresql://user:password@localhost:5432/knowthyself_dev"

# API Keys
ANTHROPIC_API_KEY=your_api_key_here

# Claude Model Configuration
# Cognitive Coach Model - handles pre-scenario warm-up questions (default: claude-3-5-haiku-20241022)
# COGNITIVE_COACH_MODEL=claude-3-5-haiku-20241022

# Core Agent Model - main paramedic scenario simulation (default: claude-sonnet-4-20250514)
# CORE_AGENT_MODEL=claude-sonnet-4-20250514

# AAR Agent Model - post-scenario reflection and learning (default: claude-3-5-haiku-20241022)
# AAR_AGENT_MODEL=claude-3-5-haiku-20241022

# Server Configuration
PORT=3001
NODE_ENV=development

# Features
ENABLE_CACHING=true
CACHE_TTL_MINUTES=5
LOG_LEVEL=info
